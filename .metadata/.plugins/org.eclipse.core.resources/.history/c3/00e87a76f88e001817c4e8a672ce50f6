package com.leetcode.test;

public class test66 {
	
	public static void main(String[] args) {
		int[] digits=new int[]{7,2,8,5,0,9,1,2,9,5,3,6,6,7,3,2,8,4,3,7,9,5,7,7,4,7,4,9,4,7,0,1,1,1,7,4,0,0,6};
		System.out.println(plusOne(digits)[9]);
	}
	
	public static int[] plusOne(int[] digits) {
		long num=0;
		//首先把该数组转化为int类型
		for(int i=digits.length-1;i>=0;i--){
			num+=digits[i]*Math.pow(10, digits.length-1-i);
		}
		num=num+1;
		String newstr=num+"";
		int size=newstr.length();
		int [] newnums=new int[size];
		int ss=0;
		for (char i : newstr.toCharArray()) {
			newnums[ss]=Integer.parseInt(i+"");
			ss++;
		}
		return newnums;
	}
}
